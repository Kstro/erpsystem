<div class="col-md-12 col-xs-12">
            <div id="pnAdd" class="hidden">
                <div class="panel panel-default">
                    <div class="panel-heading"><div class="hidden pnHeadingLabelAdd">{{pnHeadingLabelAdd}}</div><div class="hidden pnHeadingLabelEdit">{{pnHeadingLabelEdit}}</div></div>
                    <div class="panel-body">
                        <form id="frmClient"  method="post" enctype="multipart/form-data">
                        <div class="col-md-2">
                            <label>{{tratoCliente}}</label>
                            <select style="margin-top:45px !important;" id="dpbTitulo" type="text" name="titulo" class="input-sm form-control validateSelect">
                                {% for item in items %}
                                    <option value="{{item.id}}">{{item.abreviatura}}</option>
                                {% endfor %}
                            </select>
                        </div>
                        <div class="col-md-5">
                            <label>{{nombreCliente}}</label>
                            <label class="hidden requiredFields">{{requiredFields}}</label>
                            <label class="hidden emailFields">{{emailFields}}</label>
                            <input id="txtId1" type="hidden" name="id1" class="input-sm form-control">
                            <input id="txtId2" type="hidden" name="id2" class="input-sm form-control">
                            <input id="txtTipoComment" type="hidden" name="txtTipoComment" class="input-sm form-control" value="1">
                            <input id="txtName" type="text" name="nombre" class="input-sm form-control validateInput">
                            <input id="iteracion" type="hidden" name="iteracion" class="input-sm form-control">
                        </div>
                        <div class="col-md-5">
                            <label>{{apellidoCliente}}</label>
                            <input id="txtApellido" type="text" name="apellido" class="input-sm form-control validateInput">
                        </div>
                        <div class="clearfix"></div> 
                        <div class="col-md-7" style="margin-top:20px;">
                            <label>{{companiaNombreCliente}}</label>
                            <input id="txtCompania" type="text" name="compania" class="input-sm form-control validateInput">
                        </div>
                        <div class="clearfix"></div> 
                        
                       
                        <div class="col-md-5 address" style="margin-top:20px;">
                            <label>{{direccionCliente}}</label>
                            <input type="text" name="address[]" class="input-sm form-control  txtAddress txtAddressFirst">
                        </div>
                        <div class="col-md-2 zipcode" style="margin-top:20px;">
                            <label>{{zipCodeCliente}}</label>
                            <input type="text" name="zipcode[]" class="input-sm form-control  txtZipCode txtZipCodeFirst">
                        </div>
                        <div class="col-md-2 state" style="margin-top:20px;">
                            <label>{{departamentoCliente}}</label>
                            <input type="text" name="addressDepartamento[]" class="input-sm form-control txtState">
                            {#<select style="width:100%;" id name="addressDepartamento[]" class="input-sm form-control validateSelect dpbState dpbStateFirst">
                                    {% for estado in estados %}
                                        <option value={{estado.id}}>{{estado.nombre}}</option>
                                    {% endfor %}
                            </select>#}
                        </div>
                        <div class="col-md-2 city" style="margin-top:20px;">
                            <label>{{ciudadCliente}}</label>
                            <input type="text" name="addressCity[]" class="input-sm form-control txtCity">
                            {#<select style="width:100%;" id name="addressCity[]" class="input-sm form-control validateSelect dpbCity dpbCityFirst">
                                {% for ciudad in ciudades %}
                                    <option value={{ciudad.id}}>{{ciudad.nombre}}</option>
                                {% endfor %}
                            </select>#}
                        </div>
                        
                        <div class="col-md-1 addAddress" style="margin-top:20px;">
                            <button id="plusAddress" style="margin-top:25px;" class="btn btn-success"><i class="fa fa-plus"></i></button>
                        </div>
                        <div class="clearfix"></div>
                        <div class="col-md-2 phonesType" style="margin-top:20px;">
                            <label>{{phoneCliente}}</label>
                            <select style="margin-top:25px; width:100%;" type="text" name="phoneType[]" class="input-sm form-control dpbTipoPhone firstPhoneType">
                                {% for tipo in tiposTelefono %}
                                    <option value={{tipo.id}}>{{tipo.nombre}}</option>
                                {% endfor %}
                            </select>
                        </div>
                        <div class="col-md-2 phonesText" style="margin-top:20px;">
                            <input style="margin-top:25px;" type="text" name="phone[]" class="input-sm form-control txtPhone firstPhoneTxt">
                        </div>
                        <div class="col-md-2 phonesExtension" style="margin-top:20px;">
                            <label>{{extensionCliente}}</label>
                            <input type="text" name="phoneExt[]" class="input-sm form-control  txtExtension firstPhoneExtension">
                        </div>
                        <div class="col-md-1 addPhone" style="margin-top:20px;">
                            <button id="plusPhone" style="margin-top:25px;" class="btn btn-success"><i class="fa fa-plus"></i></button>
                        </div>
                        <div class="col-md-4 emailText" style="margin-top:20px;">
                            <label>{{emailCliente}}</label>
                            <input type="text" name="email[]" class="input-sm form-control  txtEmail txtEmailFirst ">
                        </div>
                        <div class="col-md-1 addEmail" style="margin-top:20px;">
                            <button id="plusEmail" style="margin-top:25px;" class="btn btn-success"><i class="fa fa-plus"></i></button>
                        </div>
                        <div class="clearfix"></div>
                        
                        <div class="col-md-12" style="margin-top:40px;">
                            <label>{{fotoCliente}}</label>
                        </div>
                        <div class="clearfix"></div> 
                        <div class="col-md-4" style="margin-top:20px;">
                            
                            <label class="hidden imageError">{{imageError}}</label>
                            <img id="imgTest" src="http://placehold.it/250x250" style="margin-bottom:25px; width:250px; height:250px; margin:auto;">
                            <br>
                            <label class="btn btn-default btn-file" style="margin-top:25px;">
                                {{inputFileLabel}} <br><input style="display:none;margin-top:25px;" type="file" name="file" id="file" class="imgProvider btn-file btn file">
                            </label>
                                
                        </div>
                        
                        {# <div class="col-md-6" style="margin-top:20px;">
                            <div class="col-md-12">
                                <div class="col-md-4">
                                    <label >{{tipoPersonaCliente}}</label>
                                </div>
                                <div class="col-md-8">
                                    <select style="margin-top:25px; width:100%;" type="text" name="persona" class="input-sm form-control validateInput dpbTipoPersona">
                                        {% for persona in personas %}
                                            <option value={{persona.id}}>{{persona.nombre}}</option>
                                        {% endfor %}
                                    </select>
                                </div>
                            </div>
                            <div class="clearfix"></div> 
                        </div> #}



                        <div class="col-md-4" style="margin-top:20px;">
                            <div class="col-md-12">

                                <div class="col-md-12">
                                        <div class="col-md-12">
                                            <label >{{estadoCliente}}</label>
                                        </div>
                                        <div class="clearfix"></div> 
                                        <div class="col-md-12">
                                            <select style="margin-top:25px; width:100%;" type="text" name="satisfaccion" class="input-sm form-control validateInput dpbTipoPersona">
                                                {% for sa in satisfaccion %}
                                                    <option value={{sa.id}}>{{sa.nombre}}</option>
                                                {% endfor %}
                                            </select>
                                        </div>
                                </div>
                            </div>
                                
                        </div>
                        
                        <div class="col-md-3 contacts" style="margin-top:20px;">
                            <label>Contacts</label>
                            <label >{{cuentaActividad}}</label>
                            <select style="width:100%;" id="contactos" name="contactos[]" class=" cal input-sm form-control validateSelect  dpbContacts dpbFirstContacts">
                                <option value="0"></option>
                                {# {% for actividad in actividades %}
                                    <option value={{actividad.id}}>{{actividad.nombre}}</option>
                                {% endfor %} #}
                            </select>
                        </div>
                        <div class="col-md-1 addContact" style="margin-top:20px;">
                            <button id="plusContact" style="margin-top:25px;" class="btn btn-success"><i class="fa fa-plus"></i></button>
                        </div> 
                        <div class="clearfix"></div> 
                        {#<div class="col-md-3" style="margin-top:20px;">
                            <label>{{nombreCliente}}</label>
                            <input id="txtContactoFirstName" type="text" name="txtContactoLastName[]" class="input-sm form-control validateInput">
                        </div>                   
                        
                        <div class="col-md-4" style="margin-top:20px;">
                            <label>{{apellidoCliente}}</label>
                            <input id="txtContactoFirstName" type="text" name="txtContactoFirstName[]" class="input-sm form-control validateInput">
                        </div>
                        <div class="col-md-2" style="margin-top:20px;">
                            <label>{{phoneCliente}}</label>
                            <input id="txtContactoLastName" type="text" name="txtContactoTelefono" class="input-sm form-control validateInput">
                        </div>
                        <div class="col-md-2" style="margin-top:20px;">
                            <label>{{emailCliente}}</label>
                            <input id="txtContactoEmail" type="text" name="txtContactoEmail" class="input-sm form-control validateInput">
                        </div>#}
                           
                        <div class="clearfix"></div> 
                            <div class="col-md-4 pull-right text-right" style="margin-top:20px;">
                                <a id="btnCancel" class="btn btn-sm btn-default">{{cancel}}</a>
                                <button id="btnSave" class="btn btn-sm btn-primary">{{save}}</button>
                            </div>
                        </div>
                        </form>
                          </div>  
                          
                    {# <div class="panel-footer">Panel Footer</div> #}
                    {% include ':crmcuenta:comments_section.html.twig' %}
            </div>
</div>
