{% extends 'base.html.twig' %}

{% block body %}
    <h1>CrmContacto list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Estado</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for crmContacto in crmContactos %}
            <tr>
                <td><a href="{{ path('admin_contact_show', { 'id': crmContacto.id }) }}">{{ crmContacto.id }}</a></td>
                <td>{{ crmContacto.estado }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('admin_contact_show', { 'id': crmContacto.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('admin_contact_edit', { 'id': crmContacto.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_contact_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
