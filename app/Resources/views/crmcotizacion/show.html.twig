{% extends 'base.html.twig' %}

{% block body %}
    <h1>Crmcotizacion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cRMCotizacion.id }}</td>
            </tr>
            <tr>
                <th>Fecharegistro</th>
                <td>{% if cRMCotizacion.fechaRegistro %}{{ cRMCotizacion.fechaRegistro|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechavencimiento</th>
                <td>{% if cRMCotizacion.fechaVencimiento %}{{ cRMCotizacion.fechaVencimiento|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechacierre</th>
                <td>{% if cRMCotizacion.fechaCierre %}{{ cRMCotizacion.fechaCierre|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Condicionesgenerales</th>
                <td>{{ cRMCotizacion.condicionesGenerales }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ cRMCotizacion.estado }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_quotes_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_quotes_edit', { 'id': cRMCotizacion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
