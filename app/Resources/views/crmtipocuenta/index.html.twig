{% extends ':layout:layouterp.html.twig' %}

{% block title -%}<title>{{tituloSales}}</title>{% endblock %}

{% block customcss %}
	
{% endblock %}

{% block descripcion %}
    <div class="col-md-5">
        <p class="robo" style="font-weight: 300; margin-bottom: 0px; font-size: 30px;">{{encabezadoSatisfaccion}}</p>
        <p class="robo" style="font-weight: 300; font-size: 14px; ">{{descripcionSatisfaccion}}</p>
    </div>

{% endblock %}

{% block acciones %}
    <div class="col-md-7 text-right panel-body">
        <button data-loading-text="Loading..." autocomplete="off" class="btnAdd btn btn-sm btn-primary btn-flat">Add</button>
        <button class="btnDelete btn btn-sm btn-danger btn-flat hide">Delete</button>
        <button class="btnShow btn btn-sm btn-success btn-flat hide">Show</button>
        <button class="btnAction btn btn-sm btn-default btn-flat hide">Action</button>
    </div>
{% endblock acciones %}

{% block navegacion %}
    <a href="/sepes/app_dev.php/admin/reporte/pacientes/reporte" class="leftnava"><li class="leftnav left ">Users management<span class="pull-right right"><i class=""></i></span></li></a>
    <a href="/sepes/app_dev.php/admin/reporte/ingresoemergencia" class="leftnava"><li class="leftnav left active"> management<span class="pull-right right"><i class="fa fa-arrow-right"></i></span></li></a>
		  
{% endblock %}

{% block contenido %}
    <div class="row">
        <div class="col-md-12 col-xs-12">
            <div id="pnAdd" class="panel panel-default" style="display:none;">
                
                <div class="panel-heading">Add</div>
                <div class="panel-body">
                    
                        <div class="col-md-6 col-xs-12">
                            <label>Name</label>
                            <input id="txtName" type="text" name="txtName" class="input-sm form-control validateInput">
                        </div>
                        <div class="col-md-6 pull-right text-right" style="margin-top:20px;">
                            <button id="btnSave" class="btn btn-sm btn-primary">Save</button>
                            <button id="btnCancel" class="btn btn-sm btn-default">Cancel</button>
                        </div>
                    
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="col-md-12 col-xs-12">
            <table id="accountTypesList" class="mdl-data-table" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th style="width: 20px; padding-left: 8px;"><input type="checkbox" name="checktodos" onclick = "seleccionarTodo();"></th> 
                        <th style="text-align: left">Name</th>                        
                    </tr>
                </thead>
            </table>
        </div>
    </div>
{% endblock %}

{% block js_extend %}
    <script type="text/javascript" src="{{ asset('Resources/validations/validation.js') }}"></script>
    <script type="text/javascript" src="{{ asset('Resources/persist/accounttype/accounttype.js') }}"></script> 
    <script type="text/javascript" src="{{ asset('Resources/persist/accounttype/funciones.js') }}"></script> 
{% endblock %}
