{% extends 'base.html.twig' %}

{% block body %}
    <h1>CtlEmpresa list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Fecharegistro</th>
                <th>Wizard</th>
                <th>Estado</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ctlEmpresa in ctlEmpresas %}
            <tr>
                <td><a href="{{ path('._admin_company_show', { 'id': ctlEmpresa.id }) }}">{{ ctlEmpresa.id }}</a></td>
                <td>{{ ctlEmpresa.nombre }}</td>
                <td>{% if ctlEmpresa.fechaRegistro %}{{ ctlEmpresa.fechaRegistro|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if ctlEmpresa.wizard %}Yes{% else %}No{% endif %}</td>
                <td>{{ ctlEmpresa.estado }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('._admin_company_show', { 'id': ctlEmpresa.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('._admin_company_edit', { 'id': ctlEmpresa.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('._admin_company_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
