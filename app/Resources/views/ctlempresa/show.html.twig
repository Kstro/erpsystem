{% extends 'base.html.twig' %}

{% block body %}
    <h1>CtlEmpresa</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ctlEmpresa.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ ctlEmpresa.nombre }}</td>
            </tr>
            <tr>
                <th>Fecharegistro</th>
                <td>{% if ctlEmpresa.fechaRegistro %}{{ ctlEmpresa.fechaRegistro|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Wizard</th>
                <td>{% if ctlEmpresa.wizard %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ ctlEmpresa.estado }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('._admin_company_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('._admin_company_edit', { 'id': ctlEmpresa.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
