{% extends 'base.html.twig' %}

{% block body %}
    <h1>Crmcaso</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ crmCaso.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ crmCaso.nombre }}</td>
            </tr>
