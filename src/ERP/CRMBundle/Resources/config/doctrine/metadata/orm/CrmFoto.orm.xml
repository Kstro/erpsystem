<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CrmFoto" table="crm_foto">
    <indexes>
      <index name="fk_crm_foto_crm_cuenta1_idx" columns="cuenta"/>
      <index name="fk_crm_foto_ctl_persona1_idx" columns="persona"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="src" type="string" column="src" length="255" nullable="false"/>
    <field name="estado" type="integer" column="estado" nullable="false"/>
    <many-to-one field="persona" target-entity="CtlPersona">
      <join-columns>
        <join-column name="persona" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="cuenta" target-entity="CrmCuenta">
      <join-columns>
        <join-column name="cuenta" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
