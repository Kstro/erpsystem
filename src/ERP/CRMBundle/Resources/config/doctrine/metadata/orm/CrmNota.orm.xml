<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CrmNota" table="crm_nota">
    <indexes>
      <index name="fk_crm_nota_ctl_usuario1_idx" columns="usuario"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="texto" type="text" column="texto" length="65535" nullable="false"/>
    <field name="fechaRegistro" type="datetime" column="fecha_registro" nullable="false"/>
    <many-to-one field="usuario" target-entity="CtlUsuario">
      <join-columns>
        <join-column name="usuario" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
